@model IEnumerable<IR_MVCwebsite.Models.results>
@{
    ViewBag.Title = "resultlinks";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>results</h1>
@{if (Model.Count()==0) {<h2>No Match results</h2> } }
<body>
    @{ int a = 0;}
    @foreach (var item in Model)
    {
        
    <div><td>
    @(a = a + 1)- 
    </td>
        <a href="@item.links">@item.links</a>
    </div>
    }
</body>


